{
    "require": {
        "php": "^7.2",
        "danielstjules/stringy": "^3.1",
        "nilportugues/sql-query-builder": "^1.8"
    },
    "require-dev": {
        "league/climate": "3.8.0",
        "rafark/phpunit-polyfills": "1.0.4",
        "mockery/mockery": "^1.5",
        "pestphp/pest": "2.2.3",
        "brain/monkey": "^2.6",
        "pestphp/pest-plugin-watch": "^2.0",
        "spatie/ignition": "1.1.0",
        "symfony/filesystem": "3.4.0",
        "rector/rector": "^0.18.6",
        "php-stubs/wordpress-stubs": "^6.3",
        "symplify/easy-coding-standard": "^12.0",
        "wp-coding-standards/wpcs": "^3.0",
        "php-stubs/woocommerce-stubs": "^8.3"
    },
    "repositories": [
         {
          "type": "package",
          "package": {
            "name": "rafark/phpunit-polyfills",
            "version": "1.0.4",
            "source": {
              "url": "https://github.com/rafark/phpunit-polyfills.git",
              "type": "git",
              "reference": "main"
            }
          }
        }
    ],
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
